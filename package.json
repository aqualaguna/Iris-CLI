{
  "name": "Iris-CLI",
  "description": "Iris CMS CLI",
  "version": "1.0.0",
  "author": "aqualaguna @aqualaguna",
  "bin": {
    "iris": "./bin/run"
  },
  "bugs": "https://github.com/aqualaguna/Iris-CLI/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/shelljs": "^0.8.8",
    "shelljs": "^0.8.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aqualaguna/Iris-CLI",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "iris",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "config": {
        "description": "manage favorite apps"
      },
      "config:test": {
        "description": "manage heroku config variables"
      }
    }
  },
  "repository": "aqualaguna/Iris-CLI",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
