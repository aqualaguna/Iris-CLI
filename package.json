{
  "name": "Iris-CLI",
  "description": "Iris CMS CLI",
  "version": "1.0.0",
  "author": "aqualaguna @aqualaguna",
  "bin": {
    "iris": "./bin/run"
  },
  "bugs": "https://github.com/aqualaguna/Iris-CLI/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/fs-extra": "^9.0.4",
    "@types/semver": "^7.3.4",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.0",
    "firebase": "^8.1.2",
    "fs-extra": "^9.0.1",
    "fs-tree-diff": "^2.0.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "shelljs": "^0.8.4",
    "tar-fs": "^2.1.1",
    "tslib": "^1",
    "uuid": "^8.3.2",
    "walk-sync": "^2.2.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aqualaguna/Iris-CLI",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "iris",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://www.npmjs.com/package/iris-cli"
    }
  },
  "repository": "aqualaguna/Iris-CLI",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
